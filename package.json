{
  "$schema": "https://json.schemastore.org/package#",
  "name": "friendlyfiregame",
  "version": "2.7.1-pre",
  "description": "Explore the world of “Friendly Fire” and meet all its inhabitants in a quest to save the world from an unknown destiny.",
  "homepage": "https://www.friendlyfiregame.com/",
  "author": "The Cat Hive Developers",
  "maintainers": [],
  "keywords": [
    "game",
    "2d",
    "browser",
    "electron",
    "platformer",
    "metroidvania",
    "pixelart"
  ],
  "license": "SEE LICENSE IN LICENSE.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/friendlyfiregame/friendlyfiregame"
  },
  "main": ".webpack/main",
  "scripts": {
    "clean": "npx rimraf lib dist out",
    "lint": "npm run -s lint:package.json && npm run -s lint:markdown && npm run -s lint:html && npm run -s lint:typescript",
    "lint:html": "npx htmlhint ./index.html",
    "lint:markdown": "npx markdownlint -c ./.markdownlint.json -i node_modules/ -i dist/ -i out/ .",
    "lint:typescript": "npx eslint --ext .ts ./src/",
    "lint:package.json": "npx npmPkgJsonLint .",
    "check": "npx jest",
    "cspell": "npx cspell \"**/*\"",
    "coverage": "npx jest --coverage",
    "jest": "npx jest",
    "test": "npm run -s cspell && npm run -s lint && npm run -s check",
    "compile": "npm run -s compile:shared && npm run -s compile:web && npm run -s compile:service-worker && npm run -s compile:electron-main && npm run -s compile:electron-preload",
    "compile:shared": "npx tsc --build ./src/shared",
    "compile:web": "npx tsc --build ./src/web",
    "compile:service-worker": "npx tsc --build ./src/service-worker",
    "compile:electron-main": "npx tsc --build ./src/electron-main",
    "compile:electron-preload": "npx tsc --build ./src/electron-preload",
    "watch": "npx webpack watch",
    "dist": "npx webpack",
    "start": "npm run -s start:dev",
    "start:dev": "npx webpack serve --mode=development --devtool=inline-source-map",
    "start:prod": "npx http-server ./dist -b -p 8001",
    "package": "npx electron-forge package",
    "package:darwin-x64": "npx electron-forge package --platform=darwin --arch=x64",
    "package:darwin-arm64": "npx electron-forge package --platform=darwin --arch=arm64",
    "package:darwin-universal": "npx electron-forge package --platform=darwin --arch=universal",
    "package:linux-x64": "npx electron-forge package --platform=linux --arch=x64",
    "package:linux-arm64": "npx electron-forge package --platform=linux --arch=arm64",
    "package:win32-x64": "npx electron-forge package --platform=win32 --arch=x64",
    "package:win32-arm64": "npx electron-forge package --platform=win32 --arch=arm64",
    "make": "npx electron-forge make",
    "prepublish": "npm run -s dist"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.1.0",
    "@electron-forge/maker-deb": "^7.1.0",
    "@electron-forge/maker-rpm": "^7.1.0",
    "@electron-forge/maker-squirrel": "^7.1.0",
    "@electron-forge/maker-zip": "^7.1.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.1.0",
    "@electron-forge/plugin-webpack": "^7.1.0",
    "@electron-forge/publisher-github": "^7.1.0",
    "@jest/globals": "^29.7.0",
    "@kayahr/jest-electron-runner": "^29.10.0",
    "@swc/register": "^0.1.10",
    "@types/express-ws": "^3.0.4",
    "@types/generate-json-webpack-plugin": "^0.3.7",
    "@types/git-rev-sync": "^2.0.2",
    "@types/webpack": "^5.28.5",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "copy-webpack-plugin": "^11.0.0",
    "cspell": "^8.0.0",
    "electron": "^27.1.2",
    "eslint": "^8.54.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "generate-json-webpack-plugin": "^2.0.0",
    "git-rev-sync": "^3.0.2",
    "git-revision-webpack-plugin": "^5.0.0",
    "html-webpack-plugin": "^5.5.3",
    "htmlhint": "^1.1.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "jest-extended": "^4.0.2",
    "markdownlint": "^0.32.1",
    "markdownlint-cli": "^0.37.0",
    "node-loader": "^2.0.0",
    "npm-package-json-lint": "^7.1.0",
    "raw-loader": "^4.0.2",
    "rimraf": "^5.0.5",
    "semver": "^7.5.4",
    "source-map-loader": "^4.0.1",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.1.0",
    "steamworks.js": "^0.3.1",
    "tslib": "^2.6.2"
  }
}
